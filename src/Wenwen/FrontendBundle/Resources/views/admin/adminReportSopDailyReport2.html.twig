{% extends 'WenwenFrontendBundle:admin:adminReportBase.html.twig' %}
{% block title %}SOP每日汇总2{% endblock %}
{% block body %}
    <div class='table-responsive'>
        <table class="table table-striped table-condensed">
            <caption>SOP每日汇总2</caption>
            <thead>
            <tr class="active text-nowrap">
                <th>日期</th>
                <th>新增问卷数</th>
                <th>关闭问卷数</th>
                <th>可回答问卷数</th>
            </tr>
            </thead>
            <tbody>
            {% for data in result %}
                <tr class="text-nowrap">
                    <td>{{ data.created_date }}</td>
                    <td>{{ data.added_count }}</td>
                    <td>{{ data.closed_count }}</td>
                    <td>{{ data.available_count }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}