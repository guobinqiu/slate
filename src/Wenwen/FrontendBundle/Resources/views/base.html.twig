<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}91问问调查网{% endblock %}</title>
    {# <meta name="viewport" content="width=1000"> #}
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1, maximum-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    {% block meta %}{% endblock %}
    <meta name="baidu_union_verify" content="464e52f63986636f61f6710be25148b2" />
    <link href="{{ asset('img/common/favicon.ico') }} " rel="shortcut icon" type="image/x-icon" />
    <link href="apple-touch-icon-iphone.png" rel="apple-touch-icon-precomposed" />
    {% block stylesheet %}{% endblock %}
    {% block global_css %}
        <link href="{{ asset('css/responsive/rpLayout.css') }} " rel="stylesheet" type="text/css">
    {% endblock %}
</head>
<body>
    {% block header %}{% endblock %}
    <div {% if app.session.has('uid') %} class="main" {% else %}  class="main unlogin" {% endif %}>
        {% block body %}{% endblock %}
    </div>
    <div class="footer">
        {% block footer %}{% endblock %}
    </div>
    {% block global_js %}
        <script type="text/javascript" src="{{ asset('js/lib/jquery-1.11.1.min.js')}}" defer></script>
        <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js')}}" defer></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}" defer></script>
        <script type="text/javascript" src="{{ asset('js/component/common.js')}}" defer></script>
        <!-- baidu statistics -->
        <div style="display:none;">
            <script type="text/javascript">
                var _bdhmProtocol = (("https:" == document.location.protocol) ? " https://" : " http://");
                document.write(unescape("%3Cscript src='" + _bdhmProtocol + "hm.baidu.com/h.js%3F878d6a1fecf76315390d7c5db8779bef' type='text/javascript'%3E%3C/script%3E"));
            </script>
        </div>
    {% endblock %}
    {% block javascript %}
    {% endblock %}
</body>
</html>