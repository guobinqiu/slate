{% extends 'WenwenFrontendBundle::base.html.twig' %}

{% block stylesheet %}
    <!-- pages css -->
    {% stylesheets filter="cssrewrite, scssphp, ?uglifycss" output="dist/css/frontend/vote.css"
        "assets/scss/frontend/component/timeline.scss"
        "assets/scss/frontend/component/form.scss"
        "assets/scss/frontend/component/page.scss"
        "assets/scss/frontend/pages/vote/*"
    %}
        <link href="{{ asset_url }}" rel="stylesheet" />
    {% endstylesheets %}
{% endblock %}

{% block javascript %}
    <!-- pages js -->
    {% javascripts filter="?uglifyjs2" output="dist/js/frontend/vote.js"
        "assets/js/frontend/component/countdown.js"
        "assets/js/frontend/component/textScroll.js"
        "assets/js/frontend/pages/vote/*"
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
