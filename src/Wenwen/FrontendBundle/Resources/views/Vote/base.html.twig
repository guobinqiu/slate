{% extends 'WenwenFrontendBundle::base.html.twig' %}

{% block stylesheet %}
    <!-- pages css -->
    {% stylesheets filter="cssrewrite, scssphp, ?uglifycss" output="dist/css/frontend/pages/help.css"
        "assets/sass/frontend/pages/help/*"
    %}
        <link href="{{ asset_url }}" rel="stylesheet" />
    {% endstylesheets %}
{% endblock %}

{% block javascript %}
    <!-- pages js -->
    {% javascripts filter="?uglifyjs2" output="dist/js/frontend/pages/vote.js"
        "assets/js/frontend/pages/vote/*"
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}