{% extends 'WenwenFrontendBundle::base.html.twig' %}

{% block stylesheet %}
    <!-- pages css -->
    {% stylesheets filter="cssrewrite, scssphp, uglifycss" output="dist/css/frontend/resetPwd.css"
        "assets/scss/frontend/layout/simple/*"
        "assets/scss/frontend/component/form.scss"
        "assets/scss/frontend/pages/user/resetPwd.scss"
    %}
        <link href="{{ asset_url }}" rel="stylesheet" />
    {% endstylesheets %}
{% endblock %}

{% block javascript %}
    <!-- pages js -->
    {% javascripts filter="?uglifyjs2" output="dist/js/frontend/resetPwd.js"
        "assets/js/frontend/pages/user/resetPwd.js"
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}