{% extends 'WenwenFrontendBundle::base.html.twig' %}

{% block stylesheet %}
    <!-- pages css -->
    {% stylesheets filter="cssrewrite, scssphp, ?uglifycss" output="dist/css/frontend/checkin.css"
		"assets/scss/frontend/component/quickLog.scss"
		"assets/scss/frontend/pages/user/login.scss"
        "assets/scss/frontend/pages/checkin.scss"
    %}
        <link href="{{ asset_url }}" rel="stylesheet" />
    {% endstylesheets %}
{% endblock %}

{% block javascript %}
    <!-- pages js -->
	{% javascripts filter="?uglifyjs2" output="dist/js/frontend/checkin.js"
        "assets/js/frontend/pages/user/loginForm.js"
        "assets/js/frontend/pages/checkin.js"
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}