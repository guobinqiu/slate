<script type="text/template" id="sop-fulcrum-research-item-template">
  <td class="first">
    <span class="trade" title="">商业问卷</span>
  </td>
  <td class="period">--</td>
  <td class="num"><%- model.getNumLabel() %></td>
  <td>
    <%- model.get('title') %>
  </td>
  <td class="answerOn">
    <a href="#" class="open-link">可回答</a>
  </td>
  <td class="point">
    <span class="num_l">
      <%- model.get('extra_info').point.complete %>
    </span>
  </td>
</script>

