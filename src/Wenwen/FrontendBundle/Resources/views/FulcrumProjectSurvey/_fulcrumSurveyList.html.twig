<input type="hidden" id="sop_app_id"   value="{{ sop_params.app_id }}" />
<input type="hidden" id="sop_app_mid"  value="{{ sop_params.app_mid }}" />
<input type="hidden" id="sop_sig"      value="{{ sop_params.sig }}" />
<input type="hidden" id="sop_time"     value="{{ sop_params.time }}" />
<input type="hidden" id="sop-api-url" value="https://{{ app_sop_hosts }}/api/v1_1/surveys/js?{{
          { 'app_id' : sop_params.app_id,
            'app_mid' : sop_params.app_mid,
            'sig' : sop_params.sig,
            'time' : sop_params.time,
            'sop_callback' :  'surveylistCallback'}|url_encode
}}">


