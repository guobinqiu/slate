{% extends 'WenwenFrontendBundle::base.html.twig' %}
{% block title %}{{ parent() }}出错啦{% endblock %}

{% block stylesheet %}
<link href="{{ asset('css/404.css') }} " rel="stylesheet" type="text/css">
{% endblock %}

{% block header %}
{% include 'WenwenFrontendBundle:Components:_header.html.twig' %}
{% endblock %}

{% block body %}
<div class="main-error-con clearfix">
	<div class="errorCon">
		<div class="errorText">
			<p class="txt">{{ error }}</p>
			<p class="btn"><a href="{{ path('_user_reg') }}">重新注册</a><a href="{{ path('help_feedback') }}">联系客服</a></p>
		</div>
	</div>
</div>
{% endblock %}