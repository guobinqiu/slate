{% extends 'WenwenFrontendBundle::base.html.twig' %}
{% block title %}{{ parent() }}问卷调查{% endblock %}
{% block meta %}
<meta name="keywords" content="问卷调查，回答问卷，问卷积分，问卷列表，商业问卷，属性问卷，问卷赚钱" />
<meta name="description" content="问卷列表展示了用户可回答的所有商业问卷、属性问卷等高积分问卷，点击可回答轻松赚取现金奖励。" />
<meta name="title" content="91问问调查网 - 问卷调查" />
{% endblock %}
{% block stylesheet %}
<link href="{{ asset('css/survey.css?v000002') }} " rel="stylesheet" type="text/css">
{% endblock %}

{% block header %}
{% include 'WenwenFrontendBundle:Components:_header.html.twig' %}
{% endblock %}

{% block body %}
	<div class="container">
		<div class="panel panel-default">
			<div class="panel-heading"><h3 class="">问卷列表</h3></div>
			<div class="panel-body">
				{% if html_survey_list|length > 0 %}
					<ul id="surveyList">
						{% for html_survey in html_survey_list %}
							<li>{{ html_survey | raw }}</li>
						{% endfor %}
					</ul>
				{% else %}
					<div id="noSurvey">
						<img src="{{asset('img/common/noSurvey.png')}}" alt="您没有新的问卷">
					</div>
				{% endif %}
			</div>
		</div>
	</div>

{% endblock %}

{% block footer %}
{% include 'WenwenFrontendBundle:Components:_footer.html.twig' %}
{% endblock %}

{% block javascript %}
<script type="text/javascript" src="{{ asset('js/plugin/jquery.cookie.js')}}"></script>
<script type="text/javascript" src="{{ asset('js/pages/home.js')}}"></script>
{% endblock %}
