<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="app.affiliate_survey_service" class="Affiliate\AppBundle\Services\AffiliateSurveyService">
            <argument type="service" id="logger" />
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>

        <service id="app.admin_project_service" class="Affiliate\AppBundle\Services\AdminProjectService">
            <argument type="service" id="logger" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="knp_paginator" />
            <argument type="service" id="router" />
        </service>

        <service id="app.admin_partner_service" class="Affiliate\AppBundle\Services\AdminPartnerService">
            <argument type="service" id="logger" />
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="knp_paginator" />
        </service>

    </services>
</container>
