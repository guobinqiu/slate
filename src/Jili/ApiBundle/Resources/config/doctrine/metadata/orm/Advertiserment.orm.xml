<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Advertiserment" table="advertiserment">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="integer" column="type" nullable="false"/>
    <field name="showFlag" type="integer" column="show_flag" nullable="false"/>
    <field name="title" type="string" column="title" length="45" nullable="true"/>
    <field name="createdTime" type="datetime" column="created_time" nullable="true"/>
    <field name="startTime" type="datetime" column="start_time" nullable="true"/>
    <field name="endTime" type="datetime" column="end_time" nullable="true"/>
    <field name="updateTime" type="datetime" column="update_time" nullable="true"/>
    <field name="content" type="string" column="content" length="10000" nullable="true"/>
    <field name="imageurl" type="string" column="imageurl" length="45" nullable="true"/>
    <field name="incentiveType" type="string" column="incentive _type" length="45" nullable="true"/>
    <field name="info" type="string" column="info" length="45" nullable="true"/>
    <field name="income" type="string" column="income" length="45" nullable="true"/>
    <field name="category" type="integer" column="category" nullable="false"/>
    <field name="deleteFlag" type="integer" column="delete_flag" nullable="true"/>
  </entity>
</doctrine-mapping>
