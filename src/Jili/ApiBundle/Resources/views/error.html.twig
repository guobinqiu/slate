{% extends 'JiliApiBundle::header.html.twig' %}
{% block title %}积粒网-页面不存在 404{% endblock %}

{% block stylesheet %}
<link href="{{ asset('css/default.css')}}" rel="stylesheet" type="text/css" />
<link href="{{ asset('css/aside.css')}}" rel="stylesheet" type="text/css" />
<link href="{{ asset('css/comingSoon.css')}}" rel="stylesheet" type="text/css" />
{% endblock %}

{% block javascript %}
<script src="{{ asset('js/banner.js')}}" type="text/javascript"></script>

{% endblock %}

{% block body %}
<div class="webArticle error404">
{% if errorMessage is defined and errorMessage is not empty %}
    <div class="error">
        <div class="errorMessage">{{ errorMessage }}</div>
        <div class=""><a href="{{ path('_homepage') }}">去首页</a></div>
    </div>
{% else %}
    <article>
    <a href="{{ path('_homepage') }}">去首页</a>
    </article>
{% endif %}
  
</div>
{% include 'JiliApiBundle::footer.html.twig' %}
{% endblock %}

