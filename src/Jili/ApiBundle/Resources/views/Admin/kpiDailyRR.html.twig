<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<HTML>
<HEAD>
<META http-equiv=Content-Type content="text/html; charset=utf8">
<LINK href="{{ asset('css/admin.css')}}" type="text/css" rel="stylesheet">
<link href="{{ asset('css/pageNav.css')}}" rel="stylesheet" type="text/css" />
<script src="{{ asset('js/lib/jquery-1.11.1.min.js')}}" type="text/javascript"></script>
<SCRIPT src="{{ asset('js/plugin/load.js')}}" type=text/javascript></SCRIPT>
<script src="{{ asset('js/plugin/My97DatePicker/WdatePicker.js')}}" type="text/javascript"></script>
<script src="{{ asset('js/kindeditor/kindeditor.js')}}" type="text/javascript"></script>
<script type="text/javascript">
$(document).ready(function(){
  $("#select").click(function (){
      window.location.href="{{ path('_admin_kpiDailyRR') }}?kpiYMD="+$('#kpiYMD').val();
  })
});
</script>
</HEAD>
<BODY>
<p class="tips">注：默认开始时间为昨天</p>
<table align="center" width="" class="search">
    <form action="{{ path('_admin_kpiDailyRR')}}" method="post" id="form1"}} >
    <tr>
        <td colspan="7" align="center">&nbsp;</td>
    </tr>
    <tr>
        <td colspan="7" align="center">开始时间:
        <input id="kpiYMD" name="kpiYMD" value="{% if kpiYMD %}{{ kpiYMD }}{% endif %}" size="18" maxlength="60" readonly="readonly" onClick="WdatePicker()" class="Wdate" style="cursor: pointer" /> &nbsp;&nbsp; 
        <input type="button" id="select" name="select" value="查 询" />&nbsp;</td>
    </tr>
    </form>
 </table>
<h3 align="center">{{ kpiYMD }}&nbsp;</h3>
   <table align="center" width="100%" border="0" class="list"> 
    <tr>
        <th align="left">RR_day</th>
        {% if kpi %}
        {% for item in kpi.rrday %}
        <td align="left">{{ item }}</td>
        {% endfor %}
        {% endif %}
    </tr>
    <tr>
        <th align="left">register_YMD</th>
        {% if kpi %}
        {% for item in kpi.registerYMD %}
        <td align="left">{{ item|date("m/d") }}</td>
        {% endfor %}
        {% endif %}
    </tr>
    <tr>
        <th align="left">register_user</th>
        {% if kpi %}
        {% for item in kpi.registerUser %}
        <td align="left">{{ item }}</td>
        {% endfor %}
        {% endif %}
    </tr>
    <tr>
        <th align="left">active_user</th>
        {% if kpi %}
        {% for item in kpi.activeUser %}
        <td align="left">{{ item }}</td>
        {% endfor %}
        {% endif %}
    </tr>
    <tr>
        <th align="left">RR</th>
        {% if kpi %}
        {% for item in kpi.rr %}
        <td align="left">{{ item }}</td>
        {% endfor %}
        {% endif %}
    </tr>
</table>
</BODY>
</HTML>