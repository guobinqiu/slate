<!doctype html>
<html>
<head>
<meta charset="UTF-8" />
<title>积粒网-小鸡找米</title>
{% include 'JiliApiBundle::seo.html.twig' %}
<!-- [if lt IE9] -->
<script src="{{ asset('js/html5.js')}}" type="text/javascript"></script>
<!-- [endif] -->
<link href="{{ asset('css/default.css')}}" rel="stylesheet" type="text/css" />
<script src="{{ asset('js/jquery-1.7.js')}}" type="text/javascript"></script>
<style>
body { margin: 0px; }
iframe {border: 0px;}
</style>
<script type="text/javascript">
function resize(){
	document.getElementById('frame3d').style.height = document.documentElement.clientHeight+"px";
}
window.onresize = resize;
</script>
</head>
<body>
{% include 'JiliApiBundle::header.html.twig' %}
{% if code %}
<div style="text-align: center;padding: 100px 0;">
<img src="{{ asset('images/error_support.gif')}}">
</div>
{% else %}
<div style="margin: 20px auto 20px; text-align:center;">
<iframe id="frame3d" name="frame3d" frameborder="0" width="1000" scrolling="auto"
	style=""  onload="this.style.height=document.documentElement.clientHeight+'px'"
	height="600" src="{{ url | raw }}"></iframe> 
</div>
{% endif %}
{% include 'JiliApiBundle::footer.html.twig' %}
<script type="text/javascript">
$(document).ready(function(){
	$(window).resize(function() {
        resizeFooter()
    });
	var resizeFooter =function(){
		$("footer").css("position","static");
	} 	
	resizeFooter();
});

</script>
</body>
</html>
