{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EmarWebsites</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Webid</th>
                <td>{{ entity.webId }}</td>
            </tr>
            <tr>
                <th>Commission</th>
                <td>{{ entity.commission }}</td>
            </tr>
            <tr>
                <th>Isdeleted</th>
                <td>{{ entity.isDeleted }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ entity.position }}</td>
            </tr>
            <tr>
                <th>Ishidden</th>
                <td>{{ entity.isHidden }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_emar_websites') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_emar_websites_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_emar_websites_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
