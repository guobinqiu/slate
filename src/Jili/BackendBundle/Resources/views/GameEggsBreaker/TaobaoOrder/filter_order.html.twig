{% extends "JiliBackendBundle::base.html.twig"%}

{% block javascripts %}
{{ parent() }}
<script src="{{ asset('js/plugin/My97DatePicker/WdatePicker.js')}}" type="text/javascript"></script>
{% endblock %}

{% block body%}

{% include("JiliBackendBundle:GameEggsBreaker/TaobaoOrder:filter_menu.html.twig") %}
<div align="center">

{% include("JiliBackendBundle::session_notification.html.twig") %}

<form action="{{ path('jili_backend_decemberactivity_orderfilter') }}" method="POST">

<div>
{{ form_label(form.beginAt)  }}
{{ form_widget(form.beginAt , {'attr': {'class': 'Wdate', 'onClick': 'WdatePicker()','style':'cursor:pointer' }}) }}
{{ form_errors(form.beginAt)  }}
</div>

<div>
{{ form_label(form.finishAt)  }}
{{ form_widget(form.finishAt , {'attr': {'class': 'Wdate', 'onClick': 'WdatePicker()','style':'cursor:pointer' }}) }}
{{ form_errors(form.finishAt)  }}
</div>

<div>
{{ form_label(form.auditStatus)  }}
{{ form_widget(form.auditStatus) }}
{{ form_errors(form.auditStatus)  }}
</div>

{{ form_rest(form) }}

<input type="submit" value="submit" />

</form>

</div>
{% endblock %}

