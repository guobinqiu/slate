sop:
survey list -> survey list api
			-> async job
			-> sop_research_survey_status_history (status: targeted)

information page -> sop_research_survey_status_history (status: init)

information button click -> sop_research_survey_status_history (status: forward)

endpage -> sop_research_survey_status_history (status: complete|screenout|quotafull)
        -> sop_research_survey_participation_history
        -> call addPoint service

script -> check

async job:
survey list api sop <-- compare --> sop_research_survey_status_history
survey list api sop -- insert --> sop_research_survey_status_history
survey list api fulcrum <-- compare --> fulcrum_research_survey_status_history
survey list api fulcrum -- insert --> fulcrum_research_survey_status_history
survey list api cint -- insert --> cint_research_survey_status_history
